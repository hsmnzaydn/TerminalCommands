<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.serkanozaydin.terminalcommands">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.category.CategoryListActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".view.activity.command.CommandListActivity" />
        <activity android:name=".view.activity.mycommand.MyCommandListActivity" />
        <activity android:name=".view.activity.createpdf.CreatePdfActivity" />
    </application>

</manifest>